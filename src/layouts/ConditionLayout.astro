---
import BaseLayout from './BaseLayout.astro'

const { frontmatter } = Astro.props
---

<BaseLayout title={frontmatter.title}>
  <div
    class="container mx-auto max-w-prose space-y-8 py-20 text-justify md:py-40">
    <h1 class="my-10 text-center text-4xl font-semibold md:text-5xl">
      Conditions Générales de la communauté
    </h1>
    <slot />
  </div>
</BaseLayout>
